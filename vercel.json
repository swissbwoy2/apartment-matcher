{
    "version": 2,
    "builds": [
        {
            "src": "app.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "app.py"
        }
    ],
    "env": {
        "FLASK_ENV": "production",
        "FLASK_APP": "app.py"
    },
    "functions": {
        "api/wix-form-submission": {
            "memory": 1024,
            "maxDuration": 10
        }
    },
    "regions": ["cdg1"],
    "public": false
}